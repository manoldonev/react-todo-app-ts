{
  "*.{html,css}": [
    "prettier --write"
  ],
  "cypress/**/*.ts": [
    "prettier --write",
    "pnpm run lint:cypress --cache --fix --cache-location ./node_modules/.cache/.eslintcache"
  ],
  "src/**/*.test.{ts,tsx}": [
    "prettier --write",
    "pnpm run lint:jest --cache --fix --cache-location ./node_modules/.cache/.eslintcache"
  ],
  "src/**/*.!(test).{ts,tsx}": [
    "prettier --write",
    "pnpm run lint:src --cache --fix --cache-location ./node_modules/.cache/.eslintcache",
    "tsc-files --noEmit",
    "pnpm run test:related"
  ]
}
